# OCR Service Environment Configuration Example

# Application Settings
APP_NAME=OCR Service
APP_VERSION=1.0.0
LOG_LEVEL=INFO

# Server Settings
HOST=0.0.0.0
PORT=8000
WORKERS=4

# OCR Settings
TESSERACT_CMD=
TESSERACT_LANG=eng
OCR_DPI=300

# File Processing Settings
MAX_FILE_SIZE_MB=10

# Performance Settings
ENABLE_CACHING=false
CACHE_TTL_SECONDS=3600
REDIS_URL=

# Security Settings
ENABLE_CORS=true
CORS_ORIGINS=["*"]
REQUIRE_TENANT_HEADER=false

# Monitoring Settings
ENABLE_METRICS=true
ENABLE_TRACING=false
